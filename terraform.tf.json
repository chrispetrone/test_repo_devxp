{
  "terraform": [
    {
      "required_providers": [
        {
          "aws": {
            "source": "hashicorp/aws",
            "version": ">= 2.7.0"
          }
        }
      ]
    }
  ],
  "provider": [
    {
      "aws": [
        {
          "region": "us-west-2"
        }
      ]
    }
  ],
  "resource": [
    {
      "aws_s3_bucket": [
        {
          "terraform_backend_bucket": [
            {
              "bucket": "terraform-state-id8nsinqt83lmdek104apsc5bj8b9swi3dobcyeyigjj1"
            }
          ]
        }
      ]
    },
    {
      "aws_instance": [
        {
          "testerino": [
            {
              "ami": "ami-0892d3c7ee96c0bf7",
              "instance_type": "t2.medium"
            }
          ]
        }
      ]
    },
    [
      {
        "aws_iam_user": [
          {
            "testerino_iam": [
              {
                "name": "testerino_iam"
              }
            ]
          }
        ]
      },
      {
        "aws_iam_user_policy_attachment": [
          {
            "testerino_iam_policy_attachment0": [
              {
                "user": "${aws_iam_user.testerino_iam.name}",
                "policy_arn": "${aws_iam_policy.testerino_policy_document.arn}"
              }
            ]
          }
        ]
      },
      {
        "aws_iam_access_key": [
          {
            "testerino_iam_access_key": [
              {
                "user": "${aws_iam_user.testerino_iam.name}"
              }
            ]
          }
        ]
      }
    ]
  ],
  "data": [
    {
      "aws_iam_policy_document": [
        {
          "testerino_policy_document": [
            {
              "statement": [
                {
                  "actions": [
                    "ec2:RunInstances",
                    "ec2:AssociateIamInstanceProfile",
                    "ec2:ReplaceIamInstanceProfileAssociation"
                  ],
                  "effect": "Allow",
                  "resources": [
                    "arn:aws:ec2:::*"
                  ]
                },
                {
                  "actions": [
                    "iam:PassRole"
                  ],
                  "effect": "Allow",
                  "resources": [
                    "${aws_instance.testerino.arn}"
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
